{
  "version": 2,
  "buildCommand": "ls -la && pwd && find . -name 'fetch_company_data.py' && python3.9 ./scripts/fetch_company_data.py && next build",
  "installCommand": "pip3 install -r requirements.txt && npm install",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
      }
    }
  ]
} 