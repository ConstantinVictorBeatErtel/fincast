{
  "version": 2,
  "buildCommand": "python3 scripts/fetch_company_data.py && next build",
  "installCommand": "pip install simfin pandas && npm install",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
      }
    }
  ]
} 